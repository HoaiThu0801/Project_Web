@{
    ViewBag.Title = "Quản lý đơn hàng";
}
<head>
    <link rel="stylesheet" type="text/css" href="~/Content/bootstrap.min.css">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">
    <link rel="stylesheet" type="text/css" href="~/Content/OrderManagement.css">
</head>

<body>
    <div class="main">
        <section class="signup">
            <div class="container">
                <div class="signup-content">
                    <div class="tab">
                        <button class="tablinks" onclick="openOrder(event, 'Store')">Cửa hàng</button>
                        <button class="tablinks" onclick="openOrder(event, 'Staff')">Nhân viên</button>
                        <button class="tablinks" onclick="openOrder(event, 'Dish')">Món ăn</button>
                    </div>
                    <div class="tabcontent" id="Store">
                    </div>
                    <div class="tabcontent" id="Staff">
                    </div>
                    <div class="tabcontent" id="Dish">
                    </div>
                </div>
            </div>
        </section>
    </div>
</body>

<script src="~/Scripts/OrderManagement.js"></script>
<script src="~/Scripts/jquery-3.5.1.min.js"></script>