@{
    ViewBag.Title = "Tạo cửa hàng";
}
@model Project_Web.Models.Store
<head>
    <link rel="stylesheet" href="~/fonts/material-icon/css/material-design-iconic-font.min.css">
    <link rel="stylesheet" href="~/Content/CreateStore.css">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">
</head>
<body>
    <div class="main">
        <section class="signup">
            <div class="container">
                <div class="signup-content">
                    <div class="tab">
                        <button class="tablinks" onclick="openForm(event, 'Store')">Cửa hàng</button>
                        <button class="tablinks" onclick="openForm(event, 'Staff')">Nhân viên</button>
                        <button class="tablinks" onclick="openForm(event, 'Dish')">Món ăn</button>
                    </div>
                    <div class="tabcontent" id="Store">
                        @{Html.RenderAction("_TabStore", "Administrator_Setting", new { page = 1 });}
                    </div>
                    <div class="tabcontent" id="Staff">
                        @*@Html.Partial("~/Views/Administrator_Setting/_TabStaff.cshtml")*@
                        @{Html.RenderAction("_TabStaff", "Administrator_Setting", new { page = 1 });}
                    </div>
                    <div class="tabcontent" id="Dish">
                        @*@Html.Partial("~/Views/Administrator_Setting/_TabDish.cshtml")*@
                        @{Html.RenderAction("_TabDish", "Administrator_Setting", new { page = 1 });}
                    </div>
                </div>
            </div>
        </section>
    </div>
</body>
<script src="~/Scripts/jquery-3.5.1.min.js"></script>
<script src="~/Scripts/jquery-3.5.1.js"></script>
<script src="~/Scripts/jquery.validate.js"></script>
<script src="~/Scripts/jquery.validate.min.js"></script>
<script src="~/Scripts/CreateStore.js"></script>